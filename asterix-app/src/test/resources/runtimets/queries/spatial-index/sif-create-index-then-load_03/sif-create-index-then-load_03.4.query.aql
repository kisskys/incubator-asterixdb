use dataverse STBench;
count(
for $x in dataset SimpleGeoPlace
let $n :=  create-rectangle(point("-4.0, 0.0"), point("1.0,4.0"))
where spatial-intersect($x.coordinates, $n)
order by $x.coordinates
return $x
);
